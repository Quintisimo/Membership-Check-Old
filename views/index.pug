  body
    h1 Membership Check
  
    form#checkUser
      label Check if person has paid membership
      input#checkStudentNumber(type='text', placeholder='Student Number', pattern='n[0-9]+', required)
      input(type='submit', value='Submit')
      
    form#addUser
      label Add person to membership database
      input#addStudentNumber(type='text', placeholder='Student Number', pattern='n[0-9]+', required)
      input#password(type='password', placeholder='Password', required)
      input(type='submit', value='Submit')
